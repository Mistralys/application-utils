{
    "name" : "mistralys/application-utils",
    "description" : "Drop-in utilities for PHP applications.",
    "type" : "library",
    "license" : "GPL-3.0-or-later",
    "authors" : 
    [
        {
            "email" : "s.mordziol@mistralys.eu",
            "name" : "Sebastian Mordziol",
            "role" : "Lead"
        }
    ],
    "autoload" : 
    {
        "psr-4" : 
        {
            "AppUtils\\" : "src/"
        },
        "files" : 
        [
            "src/functions.php"
        ],
        "classmap" : 
        [
            "src/",
            "tests/TestClasses"
        ]
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require" : 
    {
        "geshi/geshi" : ">=1.0.9.1",
        "neitanod/forceutf8" : ">=2.0.4",
        "parsecsv/php-parsecsv" : "1.2.*",
        "php": ">=7.4",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-zip": "*"
    },
    "require-dev" : 
    {
        "phpunit/phpunit" : ">=8.5",
        "phpstan/phpstan" : ">=0.12.74"
    },
    "suggest" : 
    {
        "mistralys/application-localization" : "Required to translate available texts, from date related methods or file size labels for example."
    }
}
