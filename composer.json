{
	"name" : "mistralys/application-utils",
	"description" : "Drop-in utilities for PHP applications.",
	"type" : "library",
	"license" : "GPL-3.0-or-later",
	"authors" : 
	[
		{
			"email" : "s.mordziol@mistralys.eu",
			"name" : "Sebastian Mordziol",
			"role" : "Lead"
		}
	],
	"autoload" : 
	{
		"psr-4" : 
		{
			"AppUtils\\" : "src/"
		},
        "files" : 
        [
            "src/functions.php"
        ],
		"classmap" : 
		[
			"src/"
		]
	},
	"repositories" : 
	[
		{
			"type" : "vcs",
			"url" : "https://github.com/Mistralys/HTML_QuickForm2.git"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"require" : 
	{
		"pear/html_quickform2" : "dev-trunk",
		"geshi/geshi" : "1.0.9",
		"neitanod/forceutf8" : "2.0.1",
		"parsecsv/php-parsecsv" : "0.4.5"
	},
	"require-dev" : 
	{
		"phpunit/phpunit" : "8.2.5"
	}
}